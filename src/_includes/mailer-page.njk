    <main id="scwaMain">
        <!-- HERO -->
        <section id="scwaBanner"></section>
        <!-- SCWA CONTENT -->
        <section id="scwaContent">
          <div class="container">
            <div class="row">
              <div class="col-lg"></div>
              <div class="col-lg-8">
                <h3>It's a wrap for Fort Worth, and we're ready for The Car Wash Show in Nashville.</h3>
                <p>
                  {{ offerStatement }}
                </p>
              </div>
              <div class="col-lg"></div>
            </div>
          </div>
        </section>
        <!-- Fort Worth SKYLINE -->
        <section id="dallasSkyline">
          <div class="container-fluid">
            <div class="row">
              <div class="col-lg-8">
                <img src="../img/landers/woman-dallas.png" alt="" class="img-fluid">
              </div>
              <div class="col-lg"></div>
            </div>
          </div>
        </section>
        <!-- GET STARTED SECTION -->
        <section id="get-started-promo">
            <div class="container">
                <h2 class="h1-font title text-center text-white">Car Wash Owners, Get Started With EverWash</h2>
                <p class="subtitle text-center">Interested in learning more? Fill out the form below for a demo of how the EverWash platform can increase the overall profitability of your wash.</p>

                <form name="{{affiliateName}}" action="../wash-owners-success.html" method="POST" class="inner get-started-blue" netlify-honeypot="bot-field" netlify>
                    <input id="emailSubject" type="hidden" name="Subject" aria-hidden="true">
                    <input id="MailerCampaign" type="hidden" name="MailerCampaign" value="{{affiliateName}}" aria-hidden="true">
                    <p class="lead">This form is strictly for car wash owners to inquire about EverWash services for their washes. Support questions will not be answered when using this form. For tech issues or support, please contact: <a class="text-white" href="mailto:support@everwash.com">support@everwash.com</a></p>
                    <div class="row">
                        <div class="col-lg-4 col-md-6 col-sm-3">
                            <label>Name</label>
                            <input id="name" name="name" class="form-control required" type="text" placeholder="Your Full Name" required="required" />
                        </div>

                        <div class="col-lg-4 col-md-6 col-sm-3">
                            <label>Email</label>
                            <input name="email" class="form-control required" type="email" placeholder="owner@carwash.com" required="required"/>
                        </div>

                        <div class="col-lg-4 col-md-6 col-sm-3">
                            <label>Phone</label>
                            <input name="phone" class="form-control required" type="tel" placeholder="(987) 654-3210" required="required"/>
                        </div>
                        
                    </div>
                    <div class="row">
                        <div class="col-lg"></div>
                        <div class="col-lg-4 col-md-6 col-sm-3">
                          <label>Wash Name</label>
                          <input name="wash-name" class="form-control required" type="text" placeholder="Your Wash Name" required="required"/>
                      </div>
                        <div class="col-lg-4 col-md-6 col-sm-4">
                            <label>Wash Type</label>
                            <select name="wash-type" class="form-control required" required="required">
                                <option value="" disabled="" selected="">Choose Wash Type</option>
                                <option value="Tunnel Full Service">Tunnel Full Service</option>
                                <option value="Tunnel Express Exterior">Tunnel Express Exterior</option>
                                <option value="Tunnel Flex Serve">Tunnel Flex Serve</option>
                                <option value="IBA/Rollover">IBA/Rollover</option>
                                <option value="IBA &amp; Self Serve">IBA &amp; Self Serve</option>
                                <option value="Stand Alone Self Serve">Stand Alone Self Serve</option>
                            </select>
                        </div>
                        <div class="col-lg"></div>
                    </div>
                    <input type="hidden" name="bot-field" aria-hidden="true">
                    <div class="row d-flex align-items-center justify-content-center">
                    <div class="col-lg-6 text-center">
                        <button name="submit" type="submit" id="getStartedSubmitBtn" class="btn btn-white btn-lg" onclick="submitBtn()">
                            Get Started
                        </button>
                    </div>
                </div>
                </form>
                </div>
                <div class="row">
                    <div class="col-lg"></div>
                    <div class="col-lg-7">
                        <p class="lead menu-text disclaimer-link">* This form is specifically for Car Wash Owners who wish to
                            contact EverWash about
                            setting up a membership
                            program at their location. <br> If you are a current EverWash member looking for customer support, please
                            contact our
                            Member Support Team at <a href="mailto:support@everwash.com">support@everwash.com</a> or call <a
                                href="tel:2156188808">215-618-8808</a> to speak with an agent.</p>
                    </div>
                    <div class="col-lg"></div>
                </div>
            </div>
        </section>
